#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/redst/mtv/TVM2025-public-main/node_modules/.pnpm/syncpack@13.0.0_typescript@5.7.3/node_modules/syncpack/dist/bin-update/node_modules:/home/redst/mtv/TVM2025-public-main/node_modules/.pnpm/syncpack@13.0.0_typescript@5.7.3/node_modules/syncpack/dist/node_modules:/home/redst/mtv/TVM2025-public-main/node_modules/.pnpm/syncpack@13.0.0_typescript@5.7.3/node_modules/syncpack/node_modules:/home/redst/mtv/TVM2025-public-main/node_modules/.pnpm/syncpack@13.0.0_typescript@5.7.3/node_modules:/home/redst/mtv/TVM2025-public-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/redst/mtv/TVM2025-public-main/node_modules/.pnpm/syncpack@13.0.0_typescript@5.7.3/node_modules/syncpack/dist/bin-update/node_modules:/home/redst/mtv/TVM2025-public-main/node_modules/.pnpm/syncpack@13.0.0_typescript@5.7.3/node_modules/syncpack/dist/node_modules:/home/redst/mtv/TVM2025-public-main/node_modules/.pnpm/syncpack@13.0.0_typescript@5.7.3/node_modules/syncpack/node_modules:/home/redst/mtv/TVM2025-public-main/node_modules/.pnpm/syncpack@13.0.0_typescript@5.7.3/node_modules:/home/redst/mtv/TVM2025-public-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../syncpack/dist/bin-update/index.js" "$@"
else
  exec node  "$basedir/../syncpack/dist/bin-update/index.js" "$@"
fi
